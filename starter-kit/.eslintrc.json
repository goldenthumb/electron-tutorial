{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": ["airbnb", "eslint:recommended", "plugin:react/recommended"],
    "parser": "babel-eslint",
    "rules": {
        "indent": ["error", 4],
        "react/jsx-filename-extension": 0,
        "no-console": 1,
        "no-dupe-else-if": 0,
        "no-import-assign": 0,
        "no-setter-return": 0,
        "import/no-extraneous-dependencies": [
            "error",
            {
                "devDependencies": true
            }
        ]
    },
    "plugins": ["react"]
}
